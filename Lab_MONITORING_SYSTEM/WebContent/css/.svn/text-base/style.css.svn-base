#carousel-example-generic {
    width: 100%;
    max-width: 1400px;
    margin: 0 auto;
}
html,body
{
	height:100%;
	
}
.carousel {
  height: 100%;
}
.carousel-inner > .item > img
{
	position:absolute;
	top:50px;
	left:0px;
	min-width:100%;
	height:auto;
	
}
.carousel-inner > .item
{
	height:650px;
	margin: 0 auto;

}
.carousel-caption {
    top: 200px;
    bottom: auto;
    z-index:10;
}

html,
body {
  height: 100%;
  /* The html and body elements cannot have any padding or margin. */
}

/* Wrapper for page content to push down footer */
#wrap {
  min-height: 100%;
  height: auto;
  /* Negative indent footer by its height */
  margin: 0 auto -60px;
  /* Pad bottom by footer height */
  padding: 0 0 60px;
}

/* Set the fixed height of the footer here */
#footer {
  height: 60px;
  background-color: #f5f5f5;
}

